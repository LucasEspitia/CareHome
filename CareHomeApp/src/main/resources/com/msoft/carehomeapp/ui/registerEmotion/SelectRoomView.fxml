<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" spacing="15" 
    xmlns:fx="http://javafx.com/fxml" 
    fx:controller="com.msoft.carehomeapp.ui.controllers.registerEmotion.SelectRoomController" 
    prefWidth="400" prefHeight="420"> 

    <Label text="Which room are you in?" style="-fx-font-size: 18;" /> 

    <ComboBox fx:id="comboRoom" prefWidth="200" /> 

    <Button fx:id="btnNext" text="Continue" prefWidth="180" />

</VBox>